def regex_recursion(regex, string):
    if regex == "":
        return True
    elif string == "":
        return False
    # elif regex != "." and regex != string:
    elif regex[0] != "." and regex[0] != string[0]:
        return False
    else:
        return regex_recursion(regex[1:], string[1:])


def unequal_len_regex(regex, string):
    found_match = regex_recursion(regex, string)
    if found_match:
        return True
    elif string == "":
        return False
    else:
        return unequal_len_regex(regex, string[1:])


def meta_regex(regex, string):
    if "^" in regex and "$" in regex and regex[1:-1] == string:
        return regex_recursion(regex[1:-1], string)
    elif "^" in regex:
        return regex_recursion(regex[1:], string)
    elif "$" in regex:
        return regex_recursion(regex[-2::-1], string[-1::-1])
    else:
        return unequal_len_regex(regex, string)


regex, string = input().split("|")

print(meta_regex(regex, string))
